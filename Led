#include <stdint.h>
#include "\tm4c123gh6pm.h"

void port_init(void);
void Total_distance(void);
int main(void) {
	
	port_init();
	Total_distance();
}

void Total_distance(void)
{	
	
	
int Total_distance=90;
	
	if(Total_distance<100)
		GPIO_PORTF_DATA_R &=~ 0x02;
  else
		GPIO_PORTF_DATA_R|=0x02;
}
